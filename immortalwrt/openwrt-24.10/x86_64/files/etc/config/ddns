
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'Dynu_ipv6'
	option use_ipv6 '1'
	option enabled '1'
	option lookup_host 'api.dynu.com'
	option domain 'superpassby.ddnsfree.com'
	option username 'superlu123'
	option password 'CijTbH_X*gw82WC'
	option ip_source 'interface'
	option interface 'pppoe-wan'
	option use_syslog '2'
	option check_unit 'minutes'
	option force_unit 'minutes'
	option retry_unit 'seconds'
	option update_url 'http://api.dynu.com/nic/update?hostname=[DOMAIN]&myip=no&myipv6=[IP]&username=[USERNAME]&password=[PASSWORD]'
	option check_interval '15'
	option force_interval '30'
	option ip_interface 'pppoe-wan'

